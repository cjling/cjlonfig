{
    "title": "iterm2_tmux",
    "rules": [
        {
            "description": "iterm2_tmux",
            "manipulators": [

                {
                    "type": "basic",
                    "description": "切换窗口k",
                    "from": {"key_code": "k", "modifiers": {"mandatory": ["left_shift"]}},
                    "to": [{"key_code": "a", "modifiers":["left_control"]}, {"key_code": "k"}],
                    "conditions": [{"type": "frontmost_application_if", "bundle_identifiers": ["com.googlecode.iterm2"]}]
                },


                {
                    "type": "basic",
                    "description": "切换窗口j",
                    "from": {"key_code": "j", "modifiers": {"mandatory": ["left_shift"]}},
                    "to": [{"key_code": "a", "modifiers":["left_control"]}, {"key_code": "j"}],
                    "conditions": [{"type": "frontmost_application_if", "bundle_identifiers": ["com.googlecode.iterm2"]}]
                },

                {
                    "type": "basic",
                    "description": "新窗口",
                    "from": {"key_code": "p", "modifiers": {"mandatory": ["left_command"]}},
                    "to": [{"key_code": "a", "modifiers":["left_control"]}, {"key_code": "p"}],
                    "conditions": [{"type": "frontmost_application_if", "bundle_identifiers": ["com.googlecode.iterm2"]}]
                },

                {
                    "type": "basic",
                    "description": "移动窗口k",
                    "from": {"key_code": "k", "modifiers": {"mandatory": ["left_shift", "left_control"]}},
                    "to": [{"key_code": "a", "modifiers":["left_control"]}, {"key_code": "l"}],
                    "conditions": [{"type": "frontmost_application_if", "bundle_identifiers": ["com.googlecode.iterm2"]}]
                },

                {
                    "type": "basic",
                    "description": "移动窗口j",
                    "from": {"key_code": "j", "modifiers": {"mandatory": ["left_shift", "left_control"]}},
                    "to": [{"key_code": "a", "modifiers":["left_control"]}, {"key_code": "h"}],
                    "conditions": [{"type": "frontmost_application_if", "bundle_identifiers": ["com.googlecode.iterm2"]}]
                },

                {
                    "type": "basic",
                    "description": "关闭",
                    "from": {"key_code": "w", "modifiers": {"mandatory": ["left_command"]}},
                    "to": [{"key_code": "d", "modifiers":["left_control"]}],
                    "conditions": [{"type": "frontmost_application_if", "bundle_identifiers": ["com.googlecode.iterm2"]}]
                },

                {
                    "type": "basic",
                    "description": "搜索",
                    "from": {"key_code": "f", "modifiers": {"mandatory": ["left_command"]}},
                    "to": [{"key_code": "a", "modifiers":["left_control"]}, {"key_code": "f"}],
                    "conditions": [{"type": "frontmost_application_if", "bundle_identifiers": ["com.googlecode.iterm2"]}]
                },

                {
                    "type": "basic",
                    "description": "copy-mode in",
                    "from": {"key_code": "u", "modifiers": {"mandatory": ["left_shift", "left_control"]}},
                    "to": [{"key_code": "a", "modifiers":["left_control"]}, {"key_code": "open_bracket"}],
                    "conditions": [{"type": "frontmost_application_if", "bundle_identifiers": ["com.googlecode.iterm2"]}]
                },

                {
                    "type": "basic",
                    "description": "垂直切分",
                    "from": {"key_code": "backslash", "modifiers": {"mandatory": ["left_shift", "left_control"]}},
                    "to": [{"key_code": "a", "modifiers":["left_control"]}, {"key_code": "backslash"}],
                    "conditions": [{"type": "frontmost_application_if", "bundle_identifiers": ["com.googlecode.iterm2"]}]
                },

                {
                    "type": "basic",
                    "description": "水平切分",
                    "from": {"key_code": "delete_or_backspace", "modifiers": {"mandatory": ["left_shift", "left_control"]}},
                    "to": [{"key_code": "a", "modifiers":["left_control"]}, {"key_code": "quote", "modifiers":["left_shift"]}],
                    "conditions": [{"type": "frontmost_application_if", "bundle_identifiers": ["com.googlecode.iterm2"]}]
                },

                {
                    "type": "basic",
                    "description": "panel up",
                    "from": {"key_code": "k", "modifiers": {"mandatory": ["left_command", "left_control"]}},
                    "to": [{"key_code": "a", "modifiers":["left_control"]}, {"key_code": "up_arrow"}],
                    "conditions": [{"type": "frontmost_application_if", "bundle_identifiers": ["com.googlecode.iterm2"]}]
                },

                {
                    "type": "basic",
                    "description": "panel down",
                    "from": {"key_code": "j", "modifiers": {"mandatory": ["left_command", "left_control"]}},
                    "to": [{"key_code": "a", "modifiers":["left_control"]}, {"key_code": "down_arrow"}],
                    "conditions": [{"type": "frontmost_application_if", "bundle_identifiers": ["com.googlecode.iterm2"]}]
                },
{
                    "type": "basic",
                    "description": "panel left",
                    "from": {"key_code": "h", "modifiers": {"mandatory": ["left_command", "left_control"]}},
                    "to": [{"key_code": "a", "modifiers":["left_control"]}, {"key_code": "left_arrow"}],
                    "conditions": [{"type": "frontmost_application_if", "bundle_identifiers": ["com.googlecode.iterm2"]}]
                },

                {
                    "type": "basic",
                    "description": "panel right",
                    "from": {"key_code": "l", "modifiers": {"mandatory": ["left_command", "left_control"]}},
                    "to": [{"key_code": "a", "modifiers":["left_control"]}, {"key_code": "right_arrow"}],
                    "conditions": [{"type": "frontmost_application_if", "bundle_identifiers": ["com.googlecode.iterm2"]}]
                },

                {
                    "type": "basic",
                    "description": "mouse on",
                    "from": {"key_code": "2", "modifiers": {"mandatory": ["left_command"]}},
                    "to": [{"key_code": "a", "modifiers":["left_control"]}, {"key_code": "equal_sign"}],
                    "conditions": [{"type": "frontmost_application_if", "bundle_identifiers": ["com.googlecode.iterm2"]}]
                },

                {
                    "type": "basic",
                    "description": "mouse off",
                    "from": {"key_code": "1", "modifiers": {"mandatory": ["left_command"]}},
                    "to": [{"key_code": "a", "modifiers":["left_control"]}, {"key_code": "hyphen"}],
                    "conditions": [{"type": "frontmost_application_if", "bundle_identifiers": ["com.googlecode.iterm2"]}]
                },

                {
                    "type": "basic",
                    "description": "switch",
                    "from": {"key_code": "h", "modifiers": {"mandatory": ["left_shift", "left_control"]}},
                    "to": [{"key_code": "a", "modifiers":["left_control"]}, {"key_code": "s"}],
                    "conditions": [{"type": "frontmost_application_if", "bundle_identifiers": ["com.googlecode.iterm2"]}]
                },

                {
                    "type": "basic",
                    "description": "cmd",
                    "from": {"key_code": "semicolon", "modifiers": {"mandatory": ["left_shift", "left_control"]}},
                    "to": [{"key_code": "a", "modifiers":["left_control"]}, {"key_code": "semicolon", "modifiers":["left_shift"]}],
                    "conditions": [{"type": "frontmost_application_if", "bundle_identifiers": ["com.googlecode.iterm2"]}]
                }
            ]
        }
    ]
}
