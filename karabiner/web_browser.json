{
  "title": "web browser",
  "rules": [
    {
      "description": "web browser",
      "manipulators": [
        {
          "type": "basic",

          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": [
                "left_shift"
              ]
            }
          },

          "to": [
            {
              "key_code": "tab",
                "modifiers": ["left_control"]
            }
          ],


          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                  "org.mozilla.firefox",
                  "com.google.Chrome"
              ]
            }
          ]

        },


        {
          "type": "basic",

          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": [
                "left_shift"
              ]
            }
          },

          "to": [
            {
              "key_code": "tab",
                "modifiers":[
                    "left_control",
                    "left_shift"
                ]
            }
          ],


          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                  "org.mozilla.firefox",
                  "com.google.Chrome"
              ]
            }
          ]

        }
      ]
    }
  ]
}
